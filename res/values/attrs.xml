<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Switch">
        <attr name="text_color" format="color"/>
        <attr name="inner_circle_color" format="color"/>
        <attr name="inner_circle_radius" format="dimension"/>
        <attr name="outer_circle_color" format="color"/>
        <attr name="left_text" format="string"/>
        <attr name="right_text" format="string"/>
        <attr name="text_size" format="dimension"/>
        <attr name="animate_duration" format="integer"/>
    </declare-styleable>
    <declare-styleable name="BackForward">
        <attr name="outer_color" format="color"/>
        <attr name="inner_color" format="color"/>
        <attr name="inner_radius" format="dimension"/>
        <attr name="ripple_color" format="color"/>
        <attr name="between_padding" format="dimension"/>
        <attr name="back_icon" format="reference"/>
        <attr name="back_icon_tint" format="color"/>
        <attr name="back_icon_size" format="dimension"/>
        <attr name="forward_icon" format="reference"/>
        <attr name="forward_icon_tint" format="color"/>
        <attr name="forward_icon_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="Steps">
        <attr name="steps_orientation" format="integer"/>
        <attr name="steps_count" format="integer"/>
        <attr name="step_text_color" format="color"/>
        <attr name="step_circle_color" format="color"/>
        <attr name="current_step_circle_color" format="color"/>
        <attr name="step_circle_radius" format="dimension"/>
        <attr name="step_text_size" format="dimension"/>
        <attr name="between_steps_padding" format="dimension"/>
        <attr name="current_step" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TabSteps">
        <attr name="background_color" format="color"/>
        <attr name="right_icon" format="reference"/>
        <attr name="right_icon_size" format="dimension"/>
        <attr name="tab_text_size" format="dimension"/>
        <attr name="access_color" format="color"/>
        <attr name="current_color" format="color"/>
        <attr name="denied_color" format="color"/>
        <attr name="animate_time" format="integer"/>
    </declare-styleable>
    <declare-styleable name="ListSteps">
        <attr name="background_fill" format="color"/>
        <attr name="step_radius" format="dimension"/>
        <attr name="step_back_radius" format="dimension"/>
        <attr name="mark_radius" format="dimension"/>
        <attr name="step_margin" format="dimension"/>
        <attr name="access_fill_color" format="color"/>
        <attr name="current_fill" format="color"/>
        <attr name="denied_fill_color" format="color"/>
        <attr name="access_icon_color" format="color"/>
        <attr name="denied_icon_color" format="color"/>
        <attr name="line_color" format="color"/>
        <attr name="current_drawable" format="reference"/>
        <attr name="current_drawable_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="StateBadgeButton">
        <attr name="button_icon" format="reference"/>
        <attr name="button_icon_size" format="dimension"/>
        <attr name="button_text" format="string"/>
        <attr name="button_text_size" format="dimension"/>
        <attr name="button_state" format="integer"/>
        <attr name="color_on" format="color"/>
        <attr name="color_off" format="color"/>
        <attr name="badge_radius" format="dimension"/>
        <attr name="badge_color" format="color"/>
        <attr name="badge_text_color" format="color"/>
        <attr name="badge_text_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="Dots">
        <attr name="dot_icon" format="reference"/>
        <attr name="dot_size" format="dimension"/>
        <attr name="dot_text_color" format="color"/>
        <attr name="dot_text_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="QueueDots">
        <attr name="dot_radius" format="dimension"/>
        <attr name="color_not_init" format="color"/>
        <attr name="color_init" format="color"/>
        <attr name="color_current" format="color"/>
    </declare-styleable>
</resources>